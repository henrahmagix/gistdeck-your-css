{
  "name": "Gistdeck Your CSS",
  "description": "Kick-off Gistdeck on a gist with your own CSS.",
  "version": "0.1",

  "permissions": [
    "storage",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],

  "options_page": "options.html",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Gistdeck Your CSS",
    "default_popup": "popup.html"
  },

  "manifest_version": 2
}
